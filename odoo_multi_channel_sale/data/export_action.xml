<odoo>
	<data noupdate="1" >
		<record id="export_odoo_product_template_action" model="ir.actions.act_window" >
			<field name="name">Export/Update Templates</field>
			<field name="binding_model">product.template</field>
			<field name="res_model">export.templates</field>
			<field name="view_mode">form</field>
			<field name="target">new</field>
		<record/>

		<record id="export_odoo_products_action" model="ir.actions.act_window" >
			<field name="name">Export/Update Products</field>
			<field name="binding_model">product.product</field>
			<field name="res_model">export.products</field>
			<field name="view_mode">form</field>
			<field name="target">new</field>
		<record/>

		<record id="export_odoo_products_category" model="ir.actions.act_window" >
			<field name="name">Export/Update Categories</field>
			<field name="binding_model">product.category</field>
			<field name="res_model">export.categories</field>
			<field name="view_mode">form</field>
			<field name="target">new</field>
		<record/>

