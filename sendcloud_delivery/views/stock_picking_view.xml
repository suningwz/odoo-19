<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_picking_withservicepoint_out_form" model="ir.ui.view">
        <field name="name">stock.picking.withservicepoint.form.view</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="delivery.view_picking_withcarrier_out_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='carrier_id']" position='after'>
                <field name="is_enable_service_point" invisible="1"/>
                <field name="sb_service_point_details" attrs="{'invisible':['|','|',('is_enable_service_point','=',False),('delivery_type','in', ['fixed', 'base_on_rule']),('delivery_type','=',False)]}" widget="backend_sendcloudservicepoint"/>
            </xpath>
        </field>
    </record>
</odoo>
